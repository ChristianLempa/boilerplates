---
apiVersion: v1
kind: Service
metadata:
  name: {{ resource_name }}
  namespace: {{ namespace }}
spec:
  type: {{ service_type }}
  selector:
    app.kubernetes.io/name: {{ app_selector }}
  ports:
    - name: {{ protocol | lower }}
      protocol: {{ protocol }}
      port: {{ service_port }}
      targetPort: {{ target_port }}
